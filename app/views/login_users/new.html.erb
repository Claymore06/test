<%= form_for @login_user, url: login_users_path do |f| %>
  <fieldset>
    <legend>Sign up</legend>
    <p>
      <%= f.label :user_name %>
      <%= f.text_field :user_name %>
    </p>
    <p>
      <%= f.label :password %>
      <%= f.text_field :password %>
    </p>
    <%= f.submit "sign up", class: "btn btn-primary" %>
  </fieldset>
<% end %>
